{
  "name": "mobile-assistant",
  "version": "1.0.0",
  "description": "1.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env APP_TYPE=site PORT=8080 umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "build": "umi build",
    "test": "umi test",
    "test:component": "umi test ./src/components",
    "test:all": "node ./tests/run-tests.js",
    "analyze": "cross-env ANALYZE=1 umi build",
    "lint": "eslint --ext .js src mock tests && npm run lint:style && npm run lint:prettier",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "lint:fix": "eslint --fix --ext .js src mock tests && npm run lint:style",
    "lint:prettier": "check-prettier lint",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js ",
    "tslint": "npm run tslint:fix",
    "tslint:fix": "tslint --fix 'src/**/*.ts*'",
    "precommit": "lint-staged",
    "prettier": "node ./scripts/prettier.js"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.45",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@umijs/preset-react": "^1.4.3",
    "antd-mobile": "^2.3.1",
    "better-scroll": "^1.13.4",
    "classnames": "^2.2.6",
    "dva": "^2.4.1",
    "dva-loading": "^3.0.20",
    "enzyme": "^3.8.0",
    "hash.js": "^1.1.7",
    "import-cdn-js": "^0.0.2",
    "lodash": "^4.17.11",
    "moment": "^2.23.0",
    "nprogress": "^0.2.0",
    "nzh": "^1.0.4",
    "parsec-ls": "^0.0.1",
    "path-to-regexp": "^2.4.0",
    "prop-types": "^15.6.2",
    "qs": "^6.6.0",
    "react": "^16.13.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-circle": "^1.1.1",
    "react-dom": "^16.13.0",
    "react-router-dom": "^5.1.2",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0",
    "umi": "^3.0.8",
    "weixin-js-sdk": "^1.4.0-test"
  },
  "optionalDependencies": {},
  "devDependencies": {
    "@babel/polyfill": "^7.8.7",
    "babel-eslint": "^10.1.0",
    "check-prettier": "^1.0.1",
    "cross-env": "^7.0.2",
    "cross-port-killer": "^1.0.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-umi": "^1.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-compat": "^2.6.3",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-react": "^7.12.3",
    "husky": "^1.3.1",
    "jest-puppeteer": "^3.7.0",
    "lint-staged": "^8.1.0",
    "mockjs": "^1.1.0",
    "prettier": "1.15.3",
    "stylelint": "^9.9.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.2.0",
    "uglifyjs-webpack-plugin": "^2.1.1"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,json,jsx,less}": [
      "node ./scripts/lint-prettier.js",
      "git add"
    ],
    "**/*.{js,jsx,tsx}": "npm run lint-staged:js",
    "**/*.less": "stylelint --syntax less"
  },
  "engines": {
    "node": ">=10.13"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  }
}
